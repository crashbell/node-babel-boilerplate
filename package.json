{
  "name": "node-babel-boilerplate",
  "version": "1.0.0",
  "description": "Simple node boilerplate with babel which includes steps: watch, build, test, serve",
  "main": "index.js",
  "scripts": {
    "start": "nodemon lib/index.js --exec babel-node",
    "build": "babel lib -d dist",
    "serve": "node dist/index.js",
    "test": "mocha --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crashbell/node-babel-boilerplate.git"
  },
  "keywords": [
    "node",
    "babel",
    "boilerplate"
  ],
  "author": "An Nguyen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crashbell/node-babel-boilerplate/issues"
  },
  "homepage": "https://github.com/crashbell/node-babel-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "mocha": "^3.1.0",
    "nodemon": "^1.11.0"
  }
}
